<h1>Users</h1>
<ol>
<% @users.each do |user| %>
    <li><%= link_to user.username, user_recipes_path(user) %><br>
        <ul>
            <% user.cocktails.each do |cocktail| %>
                <li><%= link_to(cocktail.name, cocktail_path(cocktail))%></li>
            <% end %>
        </ul>
    </li>
    <br>
<% end %>
</ol>