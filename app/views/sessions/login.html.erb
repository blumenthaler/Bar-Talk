<h1>Login Here</h1>

<%= form_tag('/login') do %>
    <%= label_tag "Username" %>
    <%= text_field_tag :username %>
    <br>
    <%= label_tag "Password" %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Log In" %>
<% end %>

<%= link_to "Log In with Google", '/auth/google_oauth2' %>