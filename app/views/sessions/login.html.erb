<h1>Login Here</h1>

<%= form_tag('/login') do %>
    <%= label_tag "Username" %>
    <%= text_field_tag :username %>
    <br>
    <%= label_tag "Password" %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Log In" %>
<% end %>
<br>
<%= link_to "Log In with Google", '/auth/google_oauth2' %>
<br>
<br>
<%= link_to "Home", root_path %>
<br>
<%= link_to "Signup", signup_path %>